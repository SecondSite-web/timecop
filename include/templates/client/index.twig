<!doctype html>
<html lang="en-ZA" prefix="og: http://ogp.me/ns#">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        <title>{{ post.post_title|e }}</title>

        <meta name="description" content="{{ meta.description|e }}" />
        <meta name="image" content="{{ meta.postPic|e }}" />
        <meta name="robots" content="index,follow" />
        <link rel="canonical" href="{{ site.url|e }}{{ post.post_url|e }}" />

        <meta property="og:locale" content="en_ZA" />
        <meta property="og:type" content="website" />
        <meta property="og:title" content="{{ post.post_title|e }}" />
        <meta property="og:description" content="{{ meta.description|e }}" />
        <meta property="og:url" content="{{ post.post_title|e }}" />
        <meta property="og:site_name" content="{{ site.name }}" />
        <meta property="og:image" content="{{ meta.postPic|e }}" />
        <meta property="og:image:alt" content="{{ post.post_title|e }}" />
        <meta property="og:image:type" content="image/png" />
        <meta property="fb:app_id" content="461996124627453" />
        <meta property="fb:admins" content="461996124627453" />

        <meta name="twitter:card" content="summary_large_image" />
        <meta name="twitter:title" content="{{ post.post_title|e }}" />
        <meta name="twitter:description" content="{{ meta.description|e }}" />
        <meta name="twitter:image" content="{{ meta.postPic|e }}" />
        <meta name="twitter:image:alt" content="{{ meta.description|e }}" />

        <meta name="DC.title" content="Sir Lowry's Pass" />
        <meta name="geo.region" content="ZA-WC" />
        <meta name="geo.placename" content="Sir Lowry's Pass" />
        <meta name="geo.position" content="-34.140307;18.895935" />
        <meta name="ICBM" content="-34.140307, 18.895935" />


        <link rel="apple-touch-icon" sizes="180x180" href="{{ site.url|e }}apple-touch-icon.png">
        <link rel="icon" type="image/png" sizes="32x32" href="{{ site.url|e }}favicon-32x32.png">
        <link rel="icon" type="image/png" sizes="16x16" href="{{ site.url|e }}favicon-16x16.png">
        <link rel="manifest" href="{{ site.url|e }}site.webmanifest">


        {# include your site.manifest link here with icon links for favicons #}
        {# include schema.org structured data json meta here for SEO #}
        <link rel="stylesheet" href="{{ site.url|e }}css/admin.min.css">



    </head>

    <body class="page-{{ meta.class|e }} bg-light">
    <!--[if lte IE 9]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
    <![endif]-->
    {% include 'navbar.twig' %}

    {% block header %}{% endblock header %}

    {% block body %}{% endblock body %}

        <footer>
            <a href="#" class="btn btn-primary btn-sm back-to-top move-top" role="button" title="Click to return on the top page" data-toggle="tooltip" data-placement="left">
                <i class="fa fa-chevron-up"></i>
            </a>
        </footer>
        {# for Production use #}
        <script type="text/javascript" src="{{ site.url|e }}js/admin.min.js"></script>
        {% block scripts %}{% endblock scripts %} {# for extra one page only scripts #}
    </body>
</html>
