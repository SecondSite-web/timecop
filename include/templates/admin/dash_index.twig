<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <meta name="robots" content="noindex, nofollow" />
    <title>{{ site.name }} - {{ page.title }}</title>
    <link rel="apple-touch-icon" sizes="180x180" href="{{ site.url|e }}apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="{{ site.url|e }}favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="{{ site.url|e }}favicon-16x16.png">
    <link rel="manifest" href="{{ site.url|e }}site.webmanifest">

    <link href="{{ site.url|e }}css/admin.min.css" rel="stylesheet">
    {% block head %}

    {% endblock head %}
</head>
<body id="page-top" class="page-{{ page.class }}">
<!-- Page Wrapper -->
<div id="wrapper">
    {% include 'sb_sidebar.twig' %}
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
        <!-- Main Content -->
        <div id="content">
            {% include 'sb_topnav.twig' %}
            <div class="container-fluid">
                {% block body %}

                {% endblock body %}
            </div><!-- /.container-fluid -->
        </div>
        <!-- End of Main Content -->
        <!-- Footer -->
        <footer class="sticky-footer bg-white">
            <div class="container my-auto">
                <div class="copyright text-center my-auto">
                    <span>Copyright &copy; {{ site.name }} {{ site.year }}</span>
                </div>
            </div>
        </footer>
        <!-- End of Footer -->
    </div>
    <!-- End of Content Wrapper -->
</div>
<!-- End of Page Wrapper -->
<!-- Scroll to Top Button-->
<a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
</a>
{% include 'sb_modal.twig' %}
<script type="text/javascript" src="{{ site.url|e }}js/admin.min.js"></script>

{% block scripts %}{% endblock scripts %}
</body>
</html>